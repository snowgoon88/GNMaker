<?xml version="1.0" encoding="UTF-8"?>
<configuration status="WARN">
  <appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout pattern="%-5level %class{1}.%method - %msg%n"/>
    </Console>
    <File name="File" fileName="trace.log">
      <PatternLayout pattern="%-5level %class{1}.%method - %msg%n"/>
    </File>
  </appenders>
  <loggers>
    <logger name="data.converter.DocumentConverter" level="TRACE">
    </logger>
    <logger name="gui.DocEditor" level="TRACE">
    </logger>
    <root level="WARN">
      <appender-ref ref="Console"/>
    </root>
  </loggers>
 </configuration>